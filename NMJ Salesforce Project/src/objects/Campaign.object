    <validationRules>
        <fullName>Invites_Check</fullName>
        <active>true</active>
        <description>Number of confirmations can&apos;t be greater than invites</description>
        <errorConditionFormula>Number_of_confirmations__c &gt; Number_of_Invitees__c</errorConditionFormula>
        <errorMessage>Number of Confirmations can&apos;t be greater than Invitees</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Mobile_Number_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(
IF( ISBLANK(Preferred_Mobile__c) , false , NOT(REGEX(Preferred_Mobile__c, &quot;\\D*?(\\d\\D*?){10}&quot;)) )
,
IF( ISBLANK( Alternate_Mobile__c ) , false , NOT(REGEX(Alternate_Mobile__c , &quot;\\D*?(\\d\\D*?){10}&quot;)) ))</errorConditionFormula>
        <errorMessage>Please enter valid Mobile Number</errorMessage>
    </validationRules>
